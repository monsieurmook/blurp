{
  "name": "asana-desktop",
  "productName": "Asana",
  "version": "1.10.1",
  "description": "Electron wrapper for Asana",
  "main": "generated/main/index.js",
  "scripts": {
    "build": "../../tools/web/bazel_tools/desktop/bzl_build_desktop.py",
    "clean_node_modules": "scripts/remove_npm_internal_fields.py node_modules",
    "delete_node_modules": "rm -rf ./node_modules",
    "install_clean": "npm run delete_node_modules && npm install && npm run clean_node_modules",
    "install_build": "npm install && npm run build",
    "clean_build": "npm run install_clean && npm run build",
    "build_start": "npm run build && electron-forge start -- --asana_desktop_env=dev",
    "start": "electron-forge start -- --asana_desktop_env=dev",
    "build_sand": "npm run build && electron-forge start -- --asana_desktop_env=dev --remote_host=sand",
    "sand": "electron-forge start -- --asana_desktop_env=dev --remote_host=sand",
    "make": "python scripts/make_distributables.py"
  },
  "author": "Asana, Inc.",
  "license": "UNLICENSED",
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.65",
    "@electron-forge/maker-deb": "6.0.0-beta.65",
    "@electron-forge/maker-dmg": "6.0.0-beta.65",
    "@electron-forge/maker-rpm": "6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "6.0.0-beta.65",
    "@electron-forge/maker-wix": "6.0.0-beta.65",
    "@electron-forge/maker-zip": "6.0.0-beta.65",
    "@electron/universal": "1.3.0",
    "@sentry/cli": "1.74.4",
    "@types/electron-installer-dmg": "3.0.1",
    "@types/minimist": "1.2.2",
    "@types/node": "14.18.9",
    "@types/node-fetch": "2.5.7",
    "@types/yargs": "15.0.9",
    "dotenv": "8.2.0",
    "electron": "19.0.10",
    "electron-download": "4.1.1",
    "electron-installer-dmg": "3.0.0",
    "electron-notarize": "1.1.1",
    "electron-osx-sign": "0.6.0",
    "minimist": "1.2.6",
    "prettier": "2.0.5",
    "semver": "7.3.2",
    "ts-node": "10.9.1"
  },
  "dependencies": {
    "@sentry/electron": "3.0.7",
    "electron-dl": "3.0.2",
    "node-fetch": "2.6.7",
    "yargs": "16.1.0"
  },
